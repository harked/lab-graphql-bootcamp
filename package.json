{
  "name": "harked-lab-graphql-bootcamp",
  "version": "1.0.0",
  "description": "The Modern GraphQL Bootcamp (with Node.js, Apollo, Prisma)",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harked/lab-graphql-bootcamp.git"
  },
  "keywords": [
    "graphql",
    "prisma",
    "apollo"
  ],
  "author": "Randy Harkediansa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harked/lab-graphql-bootcamp/issues"
  },
  "homepage": "https://github.com/harked/lab-graphql-bootcamp#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^9.0.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "prisma-binding": "^2.3.10"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
